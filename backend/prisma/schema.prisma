datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma_client"
}

model Event {
  id     Int      @id @default(autoincrement())
  series String
  value  String
  units  String
  date   DateTime @default(now())
  notes  String
}
